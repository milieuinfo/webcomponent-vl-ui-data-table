{
  "name": "vl-ui-data-table",
  "version": "0.0.0",
  "description": "Gebruik een data table om op een gestructureerde manier (grote hoeveelheden) relationele data te tonen. ",
  "main": "vl-data-table.js",
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-data-table.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-data-table.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-data-table/style.css' vl-data-table.js",
    "release:prepare:commit": "git add -f vl-data-table.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-data-table.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-data-table/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-data-table#readme",
  "dependencies": {
    "vl-ui-core": "1.4.2"
  },
  "peerDependencies": {
    "vl-ui-core": "^1.4.2"
  },
  "devDependencies": {
    "@govflanders/vl-ui-core": "3.7.2",
    "@govflanders/vl-ui-util": "3.7.2",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2",
    "replace": "1.0.0",
    "np": "3.0.4",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "http-server": "0.11.1",
    "sinon": "7.3.1",
    "vl-ui-util": "1.0.6"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
